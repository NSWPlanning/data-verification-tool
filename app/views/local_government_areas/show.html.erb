<% if @local_government_area.has_import? %>
  <div class="row table-full-width">
    <%= render 'local_government_areas/show/top' %>
  </div>
<% end %>

<div class="row menu-type">
  <%= render 'local_government_areas/show/mid' %>
</div>

<% if @local_government_area.has_import? || @local_government_area.has_nsi_import? %>
<div class="row">
  <div class="twelve columns">
    <div class="row">
      <h3>Latest successful import</h3>
      <%= render @local_government_area.last_successful_import  %>
    </div>
  </div>
</div>
<% end %>
