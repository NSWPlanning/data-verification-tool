<table>
  <tbody>
    <tr>
      <td>Total Records in File</td>
      <td><%= data_summary.council_file_statistics.total %></td>
    </tr>
    <tr>
      <td>In Council and LPI</td>
      <td><%= data_summary.lpi_comparison.in_both_total %></td>
    </tr>
    <tr>
      <td>Only In Council</td>
      <td><%= link_to data_summary.lpi_comparison.only_in_council_total, only_in_council_local_government_area_path(@local_government_area), id: 'only-in-council' %></td>
    </tr>
    <tr>
      <td>Only in LPI</td>
      <td><%= link_to data_summary.lpi_comparison.only_in_lpi_total, only_in_lpi_local_government_area_path(@local_government_area), id: 'only-in-lpi' %></td>
    </tr>
    <tr>
      <td>Invalid Records</td>
      <td><%= link_to data_summary.invalid_record_count, error_records_local_government_area_path(data_summary.id), id: 'invalid-records' %></td>
    </tr>
  </tbody>
</table>
